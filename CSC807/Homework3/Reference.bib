@inproceedings{begianAnalysisFuelPump2020,
  title = {Analysis of {{Fuel Pump Skimming Devices}}},
  booktitle = {Proceedings of the 2020 the 4th {{International Conference}} on {{Information System}} and {{Data Mining}}},
  author = {Begian, Charles and Kettani, Houssain},
  date = {2020-07-10},
  series = {{{ICISDM}} '20},
