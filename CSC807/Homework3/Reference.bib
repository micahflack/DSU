@inproceedings{luBADUSBCRevisitingBadUSB2021,
  title = {{{BADUSB-C}}: {{Revisiting BadUSB}} with {{Type-C}}},
  shorttitle = {{{BADUSB-C}}},
  booktitle = {2021 {{IEEE Security}} and {{Privacy Workshops}} ({{SPW}})},
  author = {Lu, Hongyi and Wu, Yechang and Li, Shuqing and Lin, You and Zhang, Chaozu and Zhang, Fengwei},
  date = {2021-05},
  pages = {327--338},
  doi = {10.1109/SPW53761.2021.00053},
