  shorttitle = {{{SoK}}},
  booktitle = {2018 {{IEEE Symposium}} on {{Security}} and {{Privacy}} ({{SP}})},
  author = {Tian, Jing and Scaife, Nolen and Kumar, Deepak and Bailey, Michael and Bates, Adam and Butler, Kevin},
