  booktitle = {Economic and {{Financial Crime}}, {{Sustainability}} and {{Good Governance}}},
  author = {Beju, Daniela-Georgeta and Făt, Codruța-Maria},
  editor = {Achim, Monica Violeta},
  date = {2023},
  series = {Contributions to {{Finance}} and {{Accounting}}},
